    dcterms:subject msc:60F10 ;
    dcterms:title "Large deviations of random variables with a finite number of approximately evaluated cumulants" ;
    dcterms:type "j" ;
    schema:identifier "0915.60042" ;
    schema:keywords <https://zbmath.org/keyword/a>,
        <https://zbmath.org/keyword/d>,
        <https://zbmath.org/keyword/e>,
        <https://zbmath.org/keyword/g>,
        <https://zbmath.org/keyword/i>,
        <https://zbmath.org/keyword/l>,
