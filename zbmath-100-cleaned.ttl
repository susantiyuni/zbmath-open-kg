    dcterms:title "The distribution of the distance to the root of the minimal subtree containing all the vertices of a given height" ;
    dcterms:type "j" ;
    schema:identifier "0807.60081" ;
    schema:keywords <https://zbmath.org/keyword/GaltonWatson_branching_process>,
        <https://zbmath.org/keyword/distribution_distance_to_the_nearest_mutual_ancestor>,
