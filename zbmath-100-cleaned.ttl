    dcterms:title "Optimal stopping rules and maximal inequalities for Bessel processes" ;
    dcterms:type "j" ;
    schema:identifier "0807.60040" ;
    schema:keywords <https://zbmath.org/keyword/Bessel_processes>,
        <https://zbmath.org/keyword/Dirichlet_processes>,
        <https://zbmath.org/keyword/local_martingales>,
        <https://zbmath.org/keyword/local_time>,
