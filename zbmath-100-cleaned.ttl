    dcterms:title "Measure compact sets of functions and consistency of statistical models" ;
    dcterms:type "j" ;
    schema:identifier "0805.62012" ;
    schema:keywords <https://zbmath.org/keyword/HewittSavage_01_law>,
        <https://zbmath.org/keyword/asymptotic_likelihood_theory>,
        <https://zbmath.org/keyword/conditional_compactness>,
