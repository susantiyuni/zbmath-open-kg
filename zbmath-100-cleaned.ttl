    schema:reviewer "None" ;
    schema:reviewerID "None" ;
    schema:url <http://epubs.siam.org/sam-bin/dbq/article/97627> .

zbmath:1191091 dcterms:creator <https://zbmath.org/author/Broniatowski_M>,
        <https://zbmath.org/author/Weber_M> ;
