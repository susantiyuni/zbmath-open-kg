    dcterms:subject msc:60H10 ;
    dcterms:title "On the existence of strong solutions of diffusion-type stochastic equations in \\(\\mathbb{R}^m\\)" ;
    dcterms:type "j" ;
    schema:identifier "0906.60049" ;
    schema:keywords <https://zbmath.org/keyword/Wiener_process>,
        <https://zbmath.org/keyword/diffusion>,
