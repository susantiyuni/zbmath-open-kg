    dcterms:title "A finite-difference algorithm for multiple moving boundary problems using real and virtual grid networks" ;
    dcterms:type "j" ;
    schema:identifier "0806.65131" ;
    schema:keywords <https://zbmath.org/keyword/Stefan_problem>,
        <https://zbmath.org/keyword/finite_difference_algorithm>,
        <https://zbmath.org/keyword/heat_transfer>,
        <https://zbmath.org/keyword/multiple_moving_boundary_problems>,
