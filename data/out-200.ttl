    dcterms:type "a" ;
    schema:identifier "0803.76021" ;
    schema:keywords <https://zbmath.org/keyword/Melnikov_techniques>,
        <https://zbmath.org/keyword/SmaleBirkhoff_homoclinic_theorem>,
