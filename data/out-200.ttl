    dcterms:title "Exact projections and the Lagrange-Galerkin method: A realistic alternative to quadrature" ;
    dcterms:type "j" ;
    schema:identifier "0809.65097" ;
    schema:keywords <https://zbmath.org/keyword/LagrangeGalerkin_method>,
        <https://zbmath.org/keyword/exact_projections>,
