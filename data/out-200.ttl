    dcterms:title "Learning via queries" ;
    dcterms:type "j" ;
    schema:identifier "0799.68155" ;
    schema:keywords <https://zbmath.org/keyword/Hilberts_tenth_problem>,
        <https://zbmath.org/keyword/Presburger_arithmetic>,
        <https://zbmath.org/keyword/decidability>,
