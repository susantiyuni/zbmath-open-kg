zbmath:622229 a schema:ScholarlyArticle ;
    rdfs:label "A ripple free sampled-data robust servomechanism controller using exponential hold" ;
    dcterms:creator <https://zbmath.org/authors/wu.yung-chun>,
        <https://zbmath.org/authors/yen.nie-zen> ;
