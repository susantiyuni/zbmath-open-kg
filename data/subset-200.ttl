    schema:keywords <https://zbmath.org/keyword/Kirchhoff_equations>,
        <https://zbmath.org/keyword/nonintegrability>,
        <https://zbmath.org/keyword/orientation>,
        <https://zbmath.org/keyword/trajectory> ;
    schema:name "Chaotic motion of a solid through ideal fluid" ;
    schema:pagination "3026-3028" ;
    schema:review [ a schema:Review ;
            schema:inLanguage "English" ;
            schema:reviewAspect "review" ;
            schema:reviewBody "The motion of a solid body through incompressible, inviscid, irrotational fluid otherwise at rest is governed by a set of ordinary differential equations called the Kirchhoff equations. Numerical evidence is presented that these equations are, in general, nonintegrable, and that chaotic motion of a solid body moving through a fluid can be detected either by monitoring changes in orientation of the body or by monitoring the trajectory in space of a fixed point of the body." ;
            schema:reviewer <https://zbmath.org/authors/petrila.titus> ] ;
    schema:url <http://hdl.handle.net/2142/112409> .

