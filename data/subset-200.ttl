zbmath:592552 a schema:ScholarlyArticle ;
    rdfs:label "On singular perturbation of the Stokes problem" ;
    dcterms:creator <https://zbmath.org/authors/kobelkov.georgy-m> ;
    dcterms:issued "1994"^^xsd:gYear ;
    dcterms:language "English" ;
