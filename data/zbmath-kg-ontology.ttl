@prefix cito: <http://purl.org/spar/cito/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix msc: <http://msc2010.org/resources/MSC/2010/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .  ### üÜï ADDED
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix zbmath: <https://zbmath.org/> .

### === CLASSES ===

schema:Organization a rdfs:Class ;
    rdfs:label "Organization" .

schema:Periodical a rdfs:Class ;
    rdfs:label "Periodical" .

schema:Person a rdfs:Class ;
    rdfs:label "Person" .

schema:Review a rdfs:Class ;
    rdfs:label "Review" .

schema:ScholarlyArticle a rdfs:Class ;
    rdfs:label "Scholarly Article" .

schema:SoftwareApplication a rdfs:Class ;
    rdfs:label "Software Application" .

schema:PropertyValue a rdfs:Class ;
    rdfs:label "Property Value" .

### ‚úÖ FIX: Introduced zbmath:DocumentType to use for article types
zbmath:DocumentType a rdfs:Class ;
    rdfs:label "Document Type" .

zbmath:doctype/book-series a zbmath:DocumentType ;
    rdfs:label "Book Series" ;
    skos:notation "b" .

zbmath:doctype/collection-article a zbmath:DocumentType ;
    rdfs:label "Collection Article" ;
    skos:notation "a" .

zbmath:doctype/journal-article a zbmath:DocumentType ;
    rdfs:label "Journal Article" ;
    skos:notation "j" .

zbmath:doctype/preprints a zbmath:DocumentType ;
    rdfs:label "Preprints" ;
    skos:notation "p" .

zbmath:ontology/msc-concept a rdfs:Class ;
    rdfs:label "MSC Concept" .

zbmath:ontology/keyword-concept a rdfs:Class ;
    rdfs:label "Keyword Concept" .

zbmath:msc-scheme a skos:ConceptScheme ;
    rdfs:label "MSC Classification" .

zbmath:keyword-scheme a skos:ConceptScheme ;
    rdfs:label "zbMATH Keywords" .

### === PROPERTIES ===

# === RDF / RDFS / SKOS ===

### üóëÔ∏è REMOVED: Already defined by RDF(S), usually not needed to redefine
# rdfs:label a rdf:Property ;
#     rdfs:domain rdfs:Resource ;
#     rdfs:range xsd:string .

rdfs:seeAlso a rdf:Property ;
    rdfs:domain skos:Concept ;
    rdfs:range rdfs:Resource .

### ‚úÖ FIX: Corrected domain from rdfs:Class ‚Üí skos:Concept
skos:notation a rdf:Property ;
    rdfs:domain skos:Concept ;
    rdfs:range xsd:string .

skos:prefLabel a rdf:Property ;
    rdfs:domain skos:Concept ;
    rdfs:range xsd:string .

skos:inScheme a rdf:Property ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:ConceptScheme .

# === DCTERMS ===

dcterms:creator a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range schema:Person .

dcterms:title a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range xsd:string .

dcterms:subject a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range zbmath:ontology/msc-concept .

dcterms:language a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range xsd:string .

dcterms:issued a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range xsd:gYear .

dcterms:publisher a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range schema:Organization .

dcterms:isPartOf a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range schema:Periodical .

### ‚úÖ FIX: Changed range from rdfs:Class ‚Üí zbmath:DocumentType
dcterms:type a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range zbmath:DocumentType .

# === SCHEMA.ORG ===

### ‚úÖ FIX: Simplified schema:name domain using owl:unionOf correctly
schema:name a rdf:Property ;
    rdfs:domain [ a owl:Class ;
        owl:unionOf (schema:ScholarlyArticle schema:Person schema:Periodical schema:Organization schema:SoftwareApplication)
    ] ;
    rdfs:range xsd:string .

schema:author a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range schema:Person .

schema:keywords a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range zbmath:ontology/keyword-concept .

schema:datePublished a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range xsd:gYear .

schema:pagination a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range xsd:string .

schema:url a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range xsd:anyURI .

schema:identifier a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range schema:PropertyValue .

schema:propertyID a rdf:Property ;
    rdfs:domain schema:PropertyValue ;
    rdfs:range xsd:string .

schema:value a rdf:Property ;
    rdfs:domain schema:PropertyValue ;
    rdfs:range xsd:string .

schema:software a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range schema:SoftwareApplication .

### ‚úÖ FIX: Generalized domain/range of isPartOf
schema:isPartOf a rdf:Property ;
    rdfs:domain rdfs:Resource ;
    rdfs:range rdfs:Resource .

schema:review a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range schema:Review .

schema:reviewBody a rdf:Property ;
    rdfs:domain schema:Review ;
    rdfs:range xsd:string .

schema:reviewer a rdf:Property ;
    rdfs:domain schema:Review ;
    rdfs:range schema:Person .

schema:reviewAspect a rdf:Property ;
    rdfs:domain schema:Review ;
    rdfs:range xsd:string .

schema:inLanguage a rdf:Property ;
    rdfs:domain schema:Review ;
    rdfs:range xsd:string .

# === CITO ===

cito:cites a rdf:Property ;
    rdfs:domain schema:ScholarlyArticle ;
    rdfs:range schema:ScholarlyArticle .

# === OPTIONAL: Mapping creator to schema:author
dcterms:creator owl:equivalentProperty schema:author .  ### üÜï ADDED

